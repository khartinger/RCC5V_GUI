,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACrD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE;YACzB,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YAChE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC/C,CAAC;QACH;MACF;IACF;EACF;AACF,CAAC","file":"G:/work3/vue/haus2/src/components/CiRepeater.vue","sourceRoot":"","sourcesContent":["<!-- CiRepeater.vue -------------------------khartinger----- -->\r\n<!-- 2022-10-19: new                                         -->\r\n<!-- 2023-01-02: import { Geo } changed                      -->\r\n\r\n<template>\r\n  <!--draw border------------------------------------------- -->\r\n  <CiBase :x=\"x\" :y=\"y\" :border=\"border\"></CiBase>\r\n  <!--draw symbol------------------------------------------- -->\r\n  <rect class=\"ciRect\"   :x=\"geo.xr()\" :y=\"geo.yr(1)\" :width=\"geo.dxi\" :height=\"geo.dyl\" />\r\n  <rect class=\"ciFillIt\" :x=\"geo.xr()\" :y=\"geo.yr(2)\" :width=\"geo.dxi\" :height=\"3 * geo.dyl\" :fill=\"colorState\"/>\r\n  <rect class=\"ciFillIt\" :x=\"geo.xr()\" :y=\"geo.yr(5)\" :width=\"geo.dxi\" :height=\"geo.dyl\" :fill=\"colorBattery\"/>\r\n  <rect class=\"ciRect\"   :x=\"geo.xr()\" :y=\"geo.yr(2)\" :width=\"geo.dxi\" :height=\"geo.dyl\" />\r\n  <!--\r\n  <line class=\"ciLine\" :x1=\"geo.xr() + geo.dxi2\" :y1=\"geo.yr(2)\" :x2=\"geo.xr() + geo.dxi2\" :y2=\"geo.yr(2) + geo.dyl\" />\r\n  -->\r\n  <!--write text-------------------------------------------- -->\r\n  <text class=\"ciFont1\" :x=\"geo.xt()\" :y=\"geo.yt(1)\">{{title}}</text>\r\n  <!--\r\n  <text class=\"ciFont1\" :x=\"geo.xt()+0.3*geo.dxi2\" :y=\"geo.yt(2)\">))</text>\r\n  -->\r\n  <text class=\"ciFont1\" :x=\"geo.xt2()+0.3*geo.dxi2\" :y=\"geo.yt(2)\">{{ iRepeaterState }}</text>\r\n\r\n  <text class=\"ciFont1\" :x=\"geo.xt()\" :y=\"geo.yt(3)\" >{{line3}}</text>\r\n  <text class=\"ciFont1\" :x=\"geo.xt()\" :y=\"geo.yt(4)\">{{line4}}</text>\r\n  <text class=\"ciFont1\" :x=\"geo.xt()\" :y=\"geo.yt(5)\">{{line5}}</text>\r\n  <!--define click area------------------------------------- -->\r\n  <rect @click=\"onClk()\" class=\"ciClick\" :x=\"geo.x0()\" :y=\"geo.y0()\" :width=\"geo.dxo\" :height=\"geo.dyo\" />\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { defineComponent } from '@vue/runtime-core'\r\nimport { Repeater, ciRepeaterController } from '@/controller/CiRepeaterController'\r\nimport CiBase from './CiBase.vue'\r\nimport { Geo } from '../classes/Geo'\r\n\r\nexport default defineComponent({\r\n  name: 'CiRepeater',\r\n  components: {\r\n    CiBase\r\n  },\r\n  data () {\r\n    return {\r\n      colorRepeater: 'lightgreen',\r\n      colorNoRepeater: 'lightgrey'\r\n    }\r\n  },\r\n  props: {\r\n    x: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    y: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    border: {\r\n      type: Number,\r\n      required: false,\r\n      default: 2\r\n    },\r\n    sid: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  computed: {\r\n    repeater: function (): Repeater | undefined {\r\n      return ciRepeaterController.repeaters.find(repeater => repeater.id === this.sid)\r\n    },\r\n    iRepeaterState: function (): number {\r\n      return this.repeater?.iRepeaterState ?? -1\r\n    },\r\n    lastRepeaterDate: function (): string {\r\n      if (this.repeater) {\r\n        if (this.iRepeaterState >= 0) return this.repeater?.lastRepeaterDate ?? this.geo.noDate\r\n      }\r\n      return this.geo.noDate\r\n    },\r\n    lastRepeaterTime: function (): string {\r\n      if (this.repeater) {\r\n        if (this.iRepeaterState >= 0) return this.repeater?.lastRepeaterTime ?? this.geo.noTime\r\n      }\r\n      return this.geo.noTime\r\n    },\r\n    // -------provide geometric data----------------------------\r\n    geo: function (): Geo {\r\n      const geo1 = new Geo(this.x, this.y)\r\n      return geo1\r\n    },\r\n    // -------text in line ...----------------------------------\r\n    title: function (): string {\r\n      if (this.repeater?.name) return this.geo.center(this.repeater.name)\r\n      return this.geo.center(this.sid)\r\n    },\r\n    line3: function (): string {\r\n      if (this.iRepeaterState < 0) return this.geo.center('unknown')\r\n      return this.geo.center(this.lastRepeaterDate)\r\n    },\r\n    line4: function (): string {\r\n      if (this.iRepeaterState < 0) return ''\r\n      return this.geo.center(this.lastRepeaterTime)\r\n    },\r\n    line5: function (): string {\r\n      if (this.repeater?.text5) return this.geo.center(this.repeater.text5)\r\n      if (this.repeater?.battery) return this.geo.center(this.repeater.battery)\r\n      return ''\r\n      // return this.geo.center(this.sid)\r\n    },\r\n    // -------color line 2 to 4 depending on iRepeaterState------\r\n    colorState: function (): string {\r\n      // console.log('colorState: ', 'iRepeaterState=' + this.iRepeaterState)\r\n      if (this.iRepeaterState === 0) return this.colorNoRepeater\r\n      if (this.iRepeaterState === 1) return this.colorRepeater\r\n      return this.geo.colorUnknown\r\n      // return 'none'\r\n    },\r\n\r\n    // -------color of rectangle 5 depending on battery value---\r\n    colorBattery: function (): string {\r\n      if (this.repeater?.text5) return 'none' // this.geo.colorOk\r\n      if (this.repeater?.battery) {\r\n        try {\r\n          const batt = parseInt(this.geo.center(this.repeater.battery).valueOf())\r\n          if (batt > this.geo.batteryMin) return this.geo.colorOk\r\n          return this.geo.colorNotOk\r\n        } catch (error) { return 'none' }\r\n      }\r\n      return this.geo.colorUnknown\r\n      // return 'none'\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    // -------react on click: publish a message (if any exists)-\r\n    onClk: function (): void {\r\n      console.log(this.sid, 'Repeater-Click')\r\n      if (this.repeater) {\r\n        if (this.repeater.pubTopic) {\r\n          let payload = '?'\r\n          const aPubTopic = this.repeater.pubTopic.split(' ')\r\n          // -publish all given topics--------------------------\r\n          aPubTopic.forEach(topic => {\r\n            if (this.repeater?.pubPayload) payload = this.repeater.pubPayload\r\n            ciRepeaterController.publishCi(topic, payload)\r\n          })\r\n        }\r\n      }\r\n    }\r\n  }\r\n})\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}