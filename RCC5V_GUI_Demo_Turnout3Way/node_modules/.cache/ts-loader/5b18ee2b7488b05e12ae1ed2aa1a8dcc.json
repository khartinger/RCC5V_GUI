CD,IAAI,GAAG,CAAC,OAAO,CAAC,MAAM,CAAC,IAAI,CAAC,EAAE;gBAC5B,EAAE,IAAI,KAAK,GAAG,EAAE,GAAG,KAAK,GAAG,CAAC,GAAG,GAAG,EAAE,CAAC,GAAG,IAAI,GAAG,CAAC,GAAG,GAAG,EAAE,CAAC,GAAG,IAAI,GAAG,EAAE,GAAG,IAAI,CAAA;aAC7E;YACD,IAAI,GAAG,CAAC,OAAO,CAAC,MAAM,CAAC,IAAI,CAAC,EAAE;gBAC5B,EAAE,IAAI,IAAI,GAAG,EAAE,GAAG,MAAM,GAAG,CAAC,GAAG,GAAG,EAAE,CAAC,GAAG,IAAI,GAAG,CAAC,GAAG,GAAG,EAAE,CAAC,GAAG,IAAI,GAAG,EAAE,GAAG,IAAI,CAAA;aAC7E;YACD,IAAI,GAAG,CAAC,OAAO,CAAC,OAAO,CAAC,IAAI,CAAC,EAAE;gBAC7B,EAAE,IAAI,GAAG,GAAG,EAAE,GAAG,OAAO,GAAG,CAAC,GAAG,GAAG,EAAE,CAAC,GAAG,IAAI,GAAG,CAAC,GAAG,GAAG,EAAE,CAAC,GAAG,IAAI,GAAG,EAAE,GAAG,IAAI,CAAA;aAC7E;YACD,OAAO,EAAE,CAAA;QACX,CAAC;KACF;IACD,OAAO,EAAE;QACP,KAAK,EAAE;YAAA,iBAYN;;YAXC,OAAO,CAAC,GAAG,CAAC,IAAI,CAAC,GAAG,EAAE,cAAc,CAAC,CAAA;YACrC,IAAM,KAAK,GAAG,iBAAiB,CAAA;YAC/B,IAAI,OAAO,GAAG,IAAI,CAAA;YAClB,IAAI,CAAC,IAAI,CAAC,MAAM;gBAAE,kBAAkB,CAAC,SAAS,CAAC,KAAK,EAAE,OAAO,CAAC,CAAA;YAC9D,UAAI,IAAI,CAAC,MAAM,0CAAE,QAAQ,EAAE;gBACzB,IAAM,SAAS,GAAG,IAAI,CAAC,MAAM,CAAC,QAAQ,CAAC,KAAK,CAAC,GAAG,CAAC,CAAA;gBACjD,SAAS,CAAC,OAAO,CAAC,UAAA,KAAK;;oBACrB,UAAI,KAAI,CAAC,MAAM,0CAAE,UAAU;wBAAE,OAAO,GAAG,KAAI,CAAC,MAAM,CAAC,UAAU,CAAA;oBAC7D,kBAAkB,CAAC,SAAS,CAAC,KAAK,EAAE,OAAO,CAAC,CAAA;gBAC9C,CAAC,CAAC,CAAA;aACH;QACH,CAAC;KACF;CACF,CAAC,CAAA","sourcesContent":["\r\nimport { defineComponent } from '@vue/runtime-core'\r\nimport { Button, ciButtonController } from '../controller/CiButtonController'\r\nimport CiBase from './CiBase.vue'\r\nimport { Geo } from '../classes/Geo'\r\n\r\nexport default defineComponent({\r\n  name: 'CiButton',\r\n  components: {\r\n    CiBase\r\n  },\r\n  props: {\r\n    x: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    y: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    border: {\r\n      type: Number,\r\n      required: false,\r\n      default: 2\r\n    },\r\n    sid: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    fx: {\r\n      type: Number,\r\n      required: false,\r\n      default: 1\r\n    },\r\n    fy: {\r\n      type: Number,\r\n      required: false,\r\n      default: 1\r\n    },\r\n    lines: {\r\n      type: String,\r\n      required: false,\r\n      default: '1'\r\n    }\r\n  },\r\n  computed: {\r\n    button: function (): Button | undefined {\r\n      return ciButtonController.buttons.find(button => button.id === this.sid)\r\n    },\r\n    iButtonState: function (): number {\r\n      return this.button?.iButtonState ?? -1\r\n    },\r\n    geo: function (): Geo {\r\n      const geo1 = new Geo(this.x, this.y)\r\n      return geo1\r\n    },\r\n    iLines: function (): number {\r\n      return parseInt(this.lines)\r\n    },\r\n    isShape: function (): boolean {\r\n      if (this.button?.shape) {\r\n        if (this.button.shape.length > 0) return true\r\n      }\r\n      return false\r\n    },\r\n    isRound: function (): boolean {\r\n      if (!this.button?.shape) return false\r\n      const sym = this.button.shape.toLowerCase()\r\n      if ((sym.indexOf('circle') >= 0) || (sym.indexOf('round') >= 0)) return true\r\n      return false\r\n    },\r\n    isSquare: function (): boolean {\r\n      if (!this.button?.shape) return false\r\n      const sym = this.button.shape.toLowerCase()\r\n      if (sym.indexOf('square') >= 0) return true\r\n      return false\r\n    },\r\n    // -------circle parameters: center, radius-----------------\r\n    cx: function (): number {\r\n      return (this.x + (this.fx - 1) * this.geo.dxo2)\r\n      // return (this.x)\r\n    },\r\n    cy: function (): number {\r\n      let y0_ = this.y\r\n      if (this.iLines === 1) y0_ = y0_ + this.geo.dyl / 2\r\n      return (y0_ + (this.fy - 1) * this.geo.dyo2)\r\n    },\r\n    cr: function (): number {\r\n      const crx_ = this.fx * this.geo.dxo2 - this.geo.dxm - 1\r\n      let cry_ = this.fy * this.geo.dyo2 - this.geo.dym - 1\r\n      if (this.iLines === 1) cry_ = cry_ - this.geo.dyl / 2\r\n      if (this.iLines === 2) cry_ = cry_ - this.geo.dyl\r\n      if (crx_ < cry_) return crx_\r\n      return cry_\r\n    },\r\n    // -------rectangle parameters: up left corner--------------\r\n    Rx0: function (): number {\r\n      if (this.isRound || this.isSquare) return (this.cx - this.cr)\r\n      return this.geo.x1()\r\n    },\r\n    // -------rectangle parameters: up left corner--------------\r\n    Ry0: function (): number {\r\n      if (this.isRound || this.isSquare) return (this.cy - this.cr)\r\n      if (this.iLines > 0) return this.geo.y1() + this.geo.dyl\r\n      return this.geo.y1()\r\n    },\r\n    // -------rectangle: corner rounding------------------------\r\n    Rrx: function (): number {\r\n      if (!this.button?.shape) return this.cr / 10\r\n      if (this.isRound) return this.cr\r\n      return (this.cr / 10)\r\n    },\r\n    Rry: function (): number {\r\n      return this.Rrx\r\n    },\r\n    // -------rectangle width-----------------------------------\r\n    Rw: function (): number {\r\n      if (this.isRound || this.isSquare) return 2 * this.cr\r\n      return this.fx * this.geo.dxo - 2 * this.geo.dxm - 1\r\n    },\r\n    // -------rectangle height----------------------------------\r\n    Rh: function (): number {\r\n      if (this.isRound || this.isSquare) return 2 * this.cr\r\n      let dyL_ = 0\r\n      if (this.iLines === 1) dyL_ = this.geo.dyl\r\n      if (this.iLines === 2) dyL_ = 2 * this.geo.dyl\r\n      return this.fy * this.geo.dyo - 2 * this.geo.dym - dyL_ - 1\r\n    },\r\n    // -------center text position depending on number of lines_\r\n    dy: function (): number {\r\n      if (this.iLines === 1) return this.geo.dyl / 2\r\n      return 0\r\n    },\r\n    // -------button representation-------------------------------\r\n    colorButton: function (): string {\r\n      if (this.button?.color) return this.button.color\r\n      if (this.iButtonState === -2) return this.geo.colorOff\r\n      if (this.iButtonState === -1) return this.geo.colorOn\r\n      if (this.iButtonState <= 0) return this.geo.colorUnknown\r\n      if (this.iButtonState === 9) return 'green' // rollomotor ab\r\n      if (this.iButtonState === 10) return 'lightgrey' // rollomotor aus\r\n      if (this.iButtonState === 11) return 'yellow' // Rollomotor auf\r\n      return this.geo.colorError\r\n    },\r\n    // -------text in line 1 and 5------------------------------\r\n    title: function (): string {\r\n      if (this.button?.name) return this.geo.center(this.button.name, this.fx)\r\n      return this.geo.center(this.sid)\r\n    },\r\n    lineLast: function (): string {\r\n      if (this.button?.textFooter) return this.geo.center(this.button.textFooter, this.fx)\r\n      if (this.button?.battery) return this.geo.center(this.button.battery, this.fx)\r\n      return this.geo.center(this.sid, this.fx)\r\n    },\r\n    // -------symbol text in line 3-----------------------------\r\n    lineCenter: function (): string {\r\n      if (this.button?.textCenter) return this.geo.center(this.button.textCenter, this.fx)\r\n      return ''\r\n    },\r\n    drawShape: function (): string {\r\n      if (!this.button?.shape) return ''\r\n      if (this.button.shape.length < 1) return ''\r\n      let s1 = ''\r\n      const sym = this.button.shape.toLowerCase()\r\n      const cR = 0.8 * this.cr //           radius of perimeter\r\n      const ca = cR * Math.sqrt(3) //       Side length of triangle\r\n      s1 = 'M' + this.cx + ',' + this.cy\r\n      if (sym.indexOf('up') >= 0) {\r\n        s1 += 'm0,-' + cR + ' l-' + (0.5 * ca) + ',' + (1.5 * cR) + ' h' + ca + ' z'\r\n      }\r\n      if (sym.indexOf('down') >= 0) {\r\n        s1 += 'm0,' + cR + ' l-' + (0.5 * ca) + ',-' + (1.5 * cR) + ' h' + ca + ' z'\r\n      }\r\n      if (sym.indexOf('left') >= 0) {\r\n        s1 += 'm-' + cR + ',0 l' + (1.5 * cR) + ',-' + (0.5 * ca) + ' v' + ca + ' z'\r\n      }\r\n      if (sym.indexOf('right') >= 0) {\r\n        s1 += 'm' + cR + ',0 l-' + (1.5 * cR) + ',-' + (0.5 * ca) + ' v' + ca + ' z'\r\n      }\r\n      return s1\r\n    }\r\n  },\r\n  methods: {\r\n    onClk: function (): void {\r\n      console.log(this.sid, 'Button-Click')\r\n      const topic = 'ci/error/button'\r\n      let payload = '-1'\r\n      if (!this.button) ciButtonController.publishCi(topic, payload)\r\n      if (this.button?.pubTopic) {\r\n        const aPubTopic = this.button.pubTopic.split(' ')\r\n        aPubTopic.forEach(topic => {\r\n          if (this.button?.pubPayload) payload = this.button.pubPayload\r\n          ciButtonController.publishCi(topic, payload)\r\n        })\r\n      }\r\n    }\r\n  }\r\n})\r\n"]}]}