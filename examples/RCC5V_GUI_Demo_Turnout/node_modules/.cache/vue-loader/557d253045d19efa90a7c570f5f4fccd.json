   if (len1 === 7) return -25\n      if (len1 === 8) return -35\n      return (-45)\n    }\n  },\n  mounted: function (): void {\n    this.intervalHandle = setInterval(() => {\n      this.timeHMS = new Date().toLocaleTimeString()\n      this.dateDM = this.fDateDM()\n    }, 1000)\n  },\n  unmounted: function (): void {\n    clearInterval(this.intervalHandle)\n  },\n  methods: {\n    fDateDM: function (): string {\n      const wochentag = ['SO', 'MO', 'DI', 'MI', 'DO', 'FR', 'SA']\n      // const s1 = 'MO 14.02.'\n      const now = new Date()\n      const s1 = wochentag[now.getDay()] + ' ' + now.getDate() + '.' + (now.getMonth() + 1) + '.'\n      // s1 = wochentag[now.getDay()] + s1\n      return s1\n    },\n    showErdgeschoss: function () {\n      console.log('CLICK - showErdgeschoss', '')\n      this.$router.push({ name: 'Erdgeschoss' })\n    }\n  }\n})\n</script>\n\n<style scoped>\n.w2Background {fill: blue; }\n.w2Font1 { font-size: 36px; font-weight: bold; fill: yellow; color: lightgreen; font-family: \"DejaVu Sans Mono\",\"monospace\"; white-space: pre; }\n</style>\n"]}]}